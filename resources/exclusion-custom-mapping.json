{
  "index_patterns": "exclusions-custom*",
  "settings": {
	"index": {
	  "refresh_interval": "5s",
	  "number_of_shards": 1,
	  "routing_partition_size": 1,
	  "number_of_replicas": 0
	}
  },
  "mappings": {
	"_routing": {
	  "required": true
	},
	"properties": {
	  "created": {
		"type": "date",
		"format": "epoch_millis"
	  },
	  "updated": {
		"type": "date",
		"format": "epoch_millis"
	  },
	  "recordType": {
        "type": "keyword"
      },
	  "groupUuid": {
		"type": "keyword"
	  },
	  "siteUuid": {
		"type": "keyword"
	  },
	  "uuid": {
		"type": "keyword"
	  },
	  "value": {
		"type": "keyword"
	  },
	  "base64": {
		"type": "keyword"
	  },
	  "type": {
		"type": "keyword"
	  },
	  "subType": {
		"type": "keyword"
	  }
	},
	"dynamic_templates": [
	  {
		"string_fields": {
		  "mapping": {
			"norms": false,
			"type": "keyword"
		  },
		  "match_mapping_type": "string",
		  "match": "*"
		}
	  }
	]
  },
  "aliases": {}
}
