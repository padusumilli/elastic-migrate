{
  "index_patterns": "audit*",
  "settings": {
	"index": {
	  "refresh_interval": "-1",
	  "number_of_shards": 1,
	  "number_of_replicas": 0
	}
  },
  "mappings": {
	"_routing": {
	  "required": true
	},
	"properties": {
	  "timestamp": {
		"type": "date",
		"format": "epoch_millis"
	  },
	  "user": {
		"type": "keyword"
	  },
	  "client": {
		"type": "keyword"
	  },
	  "service": {
		"type": "keyword"
	  },
	  "siteUuid": {
		"type": "keyword"
	  },
	  "object": {
		"type": "keyword"
	  },
	  "op": {
		"type": "keyword"
	  },
	  "status": {
		"type": "integer"
	  }
	},
	"dynamic_templates": [
	  {
		"string_fields": {
		  "mapping": {
			"norms": false,
			"type": "keyword"
		  },
		  "match_mapping_type": "string",
		  "match": "*"
		}
	  }
	]
  },
  "aliases": {}
}
